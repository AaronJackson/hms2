{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.4",
        "ext-redis": "^5.3",
        "akaunting/money": "^1.0",
        "beberlei/doctrineextensions": "^1.0",
        "bogardo/mailgun": "^5.2",
        "consoletvs/charts": "^6.5",
        "digbang/safe-queue": "0.4.*",
        "fideloper/proxy": "^4.0",
        "garygreen/pretty-routes": "^1.0",
        "gedmo/doctrine-extensions": "^2.4",
        "guzzlehttp/guzzle": "^6.3",
        "karpy47/php-mqtt-client": "^1.0.4",
        "laracasts/flash": "^3.0",
        "laravel-doctrine/acl": "1.1.*",
        "laravel-doctrine/extensions": "1.1.*",
        "laravel-doctrine/migrations": "1.3.*",
        "laravel-doctrine/orm": "1.5.*",
        "laravel/framework": "^6.0",
        "laravel/horizon": "^3.1",
        "laravel/passport": "^9.3.2",
        "laravel/slack-notification-channel": "^2.0",
        "laravel/tinker": "^2.0",
        "lwk/vimbadmin-laravel-client": "^1.1.3",
        "php-http/guzzle6-adapter": "^1.1",
        "soundasleep/html2text": "^1.1",
        "spatie/laravel-cookie-consent": "^2.0",
        "spatie/laravel-stripe-webhooks": "^2.0",
        "stripe/stripe-php": "^6.35",
        "tightenco/ziggy": "^0.8.1",
        "tremby/laravel-git-version": "^1.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "behat/behat": "^3.0",
        "behat/mink-extension": "^2.1",
        "beyondcode/laravel-dump-server": "^1.0",
        "facade/ignition": "^1.4",
        "filp/whoops": "^2.0",
        "fakerphp/faker": "^1.9.1",
        "laracasts/behat-laravel-extension": "dev-master",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "HMS\\": "app/HMS"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
